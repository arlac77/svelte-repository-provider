{
  "name": "svelte-repository-provider",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "svelte": "./src/index.svelte",
  "description": "Svelte components for repository providers",
  "keywords": [
    "component",
    "repository",
    "svelte",
    "web"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "start": "rollup -c tests/app/rollup.config.mjs -w",
    "test": "npm run test:cafe",
    "test:cafe": "testcafe $BROWSER:headless tests/cafe/*.js -s build/test --page-request-timeout 9000 --app-init-delay 3000 --app \"rollup -c tests/app/rollup.config.mjs -w\"",
    "docs": "documentation readme --section=API ./src/**/*.mjs",
    "lint": "documentation lint ./src/index.mjs && npm run lint:docs",
    "lint:docs": "documentation lint ./src/**/*.mjs"
  },
  "dependencies": {
    "repository-provider": "^28.0.0",
    "svelte-guard-history-router": "^4.6.3"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.2.0",
    "@rollup/plugin-virtual": "^2.1.0",
    "content-entry": "^4.1.9",
    "documentation": "^13.2.5",
    "mf-styling": "arlac77/mf-styling",
    "postcss": "^8.4.12",
    "postcss-import": "^14.1.0",
    "rollup": "^2.70.1",
    "rollup-plugin-dev": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "^7.1.0",
    "semantic-release": "^19.0.2",
    "svelte": "^3.47.0",
    "testcafe": "^1.18.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/svelte-repository-provider.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/svelte-repository-provider/issues"
  },
  "homepage": "https://github.com/arlac77/svelte-repository-provider#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-svelte-component"
    ]
  }
}
